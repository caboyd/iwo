{
  "name": "iwo.ts",
  "sideEffects": false,
  "version": "0.2.1",
  "description": "Typescript Webgl2 Physically Based Renderer",
  "main": "dist/iwo.js",
  "scripts": {
    "watch": "webpack --progress --colors --watch --mode=production",
    "build": "webpack --progress --colors --mode=production",
    "builddev": "webpack --progress --colors --mode=development",
    "dev": "webpack-dev-server --content-base ./ --colors --hot --inline --mode=development --env.devServer",
    "gh-publish": "npm run build&&cd dist&&git add --all&&git commit -m \"Deploy to gh-pages\"&&git push origin gh-pages&&cd.."
  },
  "author": "Chris Boyd",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "prettier": {
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 120,
    "parser": "typescript",
    "semi": true,
    "trailingComma": "es5"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/gl-matrix": "^2.4.5",
    "@types/node": "^13.7.4",
    "@types/webgl2": "0.0.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "file-loader": "^5.1.0",
    "gl-matrix": "^3.0.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "string-replace-loader": "^2.2.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "peerDependencies": {
    "gl-matrix": "^3.2.1"
  }
}
